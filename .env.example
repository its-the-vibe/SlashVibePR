# SlashVibePR — example environment variables
# Copy this file to .env and fill in the real values.
# Do NOT commit .env to source control.

# Redis
REDIS_ADDR=host.docker.internal:6379
REDIS_PASSWORD=

# Redis channel/list names (defaults shown — only override if needed)
REDIS_CHANNEL=slack-commands
REDIS_VIEW_SUBMISSION_CHANNEL=slack-relay-view-submission
REDIS_POPPIT_LIST=poppit:commands
REDIS_POPPIT_OUTPUT_CHANNEL=poppit:command-output
REDIS_SLACKLINER_LIST=slack_messages

# Slack
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token-here
SLACK_CHANNEL_ID=C0123456789

# Logging: DEBUG | INFO | WARN | ERROR
LOG_LEVEL=INFO
